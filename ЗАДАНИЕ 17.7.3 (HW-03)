money = float(input ('Введите сумму вклада -'))
per_cent = {'ТКБ': 5.6, 'СКБ':5.9, 'ВТБ':4.28, 'СБЕР':4.0}
d = list (per_cent.values()) #Перевод словаря в список
deposit = [round((i * money)/100, 2) for i in d] #Арифметическое действие на каждый элемент из списка + округление до 2х знаков
max_deposit = float(max(deposit)) #Определение максимального значения в списке
print ('deposit:', deposit,
       '\nМаксимальная сумма, которую вы можете заработать — deposit:', round(max_deposit, 2)) #округление до 2х знаков
